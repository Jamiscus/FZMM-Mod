<owo-ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/wisp-forest/owo-lib/1.21/owo-ui.xsd">
    <components>
        <fzmm.styled-flow-layout direction="horizontal">
            <children>
                <fzmm.styled-label id="title">
                    <positioning type="absolute">20,20</positioning>
                    <text translate="true">fzmm.gui.title.headGallery</text>
                </fzmm.styled-label>


                <fzmm.styled-flow-layout direction="horizontal">
                    <children>
                        <fzmm.styled-flow-layout direction="vertical" id="options-list">
                            <children>

                                <fzmm.styled-flow-layout direction="vertical">
                                    <children>
                                        <fzmm.styled-label>
                                            <text translate="true">fzmm.gui.headGallery.option.tag-search</text>
                                            <tooltip-text translate="true">fzmm.gui.headGallery.option.tag-search.tooltip</tooltip-text>
                                        </fzmm.styled-label>
                                        <text-box id="content-search">
                                            <sizing>
                                                <horizontal method="fill">100</horizontal>
                                            </sizing>
                                        </text-box>
                                    </children>

                                    <horizontal-alignment>left</horizontal-alignment>
                                    <gap>4</gap>

                                    <sizing>
                                        <horizontal method="fill">100</horizontal>
                                        <vertical method="fixed">36</vertical>
                                    </sizing>

                                    <padding>
                                        <right>8</right>
                                    </padding>
                                </fzmm.styled-flow-layout>

                                <template name="left-option-list">
                                    <id>tags-layout</id>
                                    <labelId>tags</labelId>

                                    <vertical-sizing-method>fixed</vertical-sizing-method>
                                    <vertical-sizing-value>46</vertical-sizing-value>
                                </template>

                                <fzmm.styled-flow-layout direction="vertical">
                                    <children>
                                        <template name="left-option-list">
                                            <id>minecraft-heads-category-list</id>
                                            <labelId>category</labelId>

                                            <vertical-sizing-method>fill</vertical-sizing-method>
                                            <vertical-sizing-value>100</vertical-sizing-value>
                                        </template>
                                    </children>
                                    <margins>
                                        <top>80</top>
                                    </margins>
                                    <positioning type="relative">0,0</positioning>

                                    <sizing>
                                        <vertical method="fill">100</vertical>
                                    </sizing>

                                </fzmm.styled-flow-layout>

                            </children>

                            <gap>6</gap>

                            <sizing>
                                <horizontal method="fill">30</horizontal>
                                <vertical method="fill">70</vertical>
                            </sizing>

                        </fzmm.styled-flow-layout>

                    </children>
                </fzmm.styled-flow-layout>

                <fzmm.styled-flow-layout direction="vertical" id="gallery-layout">
                    <children>
                        <fzmm.styled-flow-layout direction="horizontal">
                            <children>
                                <button id="previous-page-button">
                                    <positioning type="relative">0,100</positioning>
                                    <text translate="true">fzmm.gui.button.arrow2.left</text>
                                </button>
                                <fzmm.styled-label id="current-page-label">
                                    <text>0/0</text>
                                </fzmm.styled-label>
                                <button id="next-page-button">
                                    <positioning type="relative">100,100</positioning>
                                    <text translate="true">fzmm.gui.button.arrow2.right</text>
                                </button>
                            </children>

                            <vertical-alignment>center</vertical-alignment>
                            <horizontal-alignment>center</horizontal-alignment>

                            <sizing>
                                <horizontal method="fill">100</horizontal>
                                <vertical method="fixed">22</vertical>
                            </sizing>
                            <margins>
                                <all>1</all>
                            </margins>

                            <positioning type="relative">0,100</positioning>
                        </fzmm.styled-flow-layout>

                        <fzmm.styled-flow-layout direction="vertical">
                            <children>
                                <fzmm.styled-label id="error-message">

                                    <margins>
                                        <all>4</all>
                                    </margins>

                                    <positioning type="relative">0,0</positioning>

                                    <sizing>
                                        <horizontal method="expand">100</horizontal>
                                    </sizing>
                                </fzmm.styled-label>

                                <fzmm.styled-scroll direction="vertical" id="content-scroll">
                                    <fzmm.styled-flow-layout direction="ltr-text-flow" id="content">
                                        <children>

                                        </children>

                                        <sizing>
                                            <horizontal method="fill">100</horizontal>
                                        </sizing>

                                        <vertical-alignment>top</vertical-alignment>
                                        <horizontal-alignment>left</horizontal-alignment>
                                    </fzmm.styled-flow-layout>


                                    <sizing>
                                        <vertical method="fill">100</vertical>
                                    </sizing>

                                </fzmm.styled-scroll>
                            </children>

                            <padding>
                                <bottom>25</bottom>
                            </padding>

                            <sizing>
                                <vertical method="fill">100</vertical>
                            </sizing>

                        </fzmm.styled-flow-layout>
                    </children>

                    <sizing>
                        <horizontal method="fill">52</horizontal>
                        <vertical method="fill">70</vertical>
                    </sizing>

                    <surface>
                        <panel dark="true"/>
                    </surface>

                    <padding>
                        <all>6</all>
                    </padding>

                    <horizontal-alignment>center</horizontal-alignment>
                    <vertical-alignment>center</vertical-alignment>
                </fzmm.styled-flow-layout>

                <fzmm.styled-flow-layout direction="vertical" id="preview-layout">
                    <children>
                    </children>

                    <sizing>
                        <horizontal method="fixed">48</horizontal>
                        <vertical method="fill">70</vertical>
                    </sizing>
                </fzmm.styled-flow-layout>

                <fzmm.styled-flow-layout direction="horizontal">
                    <children>

                        <fzmm.styled-flow-layout direction="horizontal">
                            <children>

                                <button id="minecraft-heads-button">
                                    <text translate="true">fzmm.gui.headGallery.button.minecraftHeads</text>
                                </button>

                                <button id="back-button">
                                    <text translate="true">fzmm.gui.button.back</text>
                                </button>

                            </children>

                            <gap>8</gap>

                            <margins>
                                <right>20</right>
                                <bottom>10</bottom>
                            </margins>
                        </fzmm.styled-flow-layout>

                    </children>

                    <positioning type="relative">100,100</positioning>

                    <vertical-alignment>bottom</vertical-alignment>
                    <horizontal-alignment>right</horizontal-alignment>
                </fzmm.styled-flow-layout>
            </children>


            <vertical-alignment>center</vertical-alignment>
            <horizontal-alignment>center</horizontal-alignment>

            <surface>
                <vanilla-translucent/>
            </surface>
        </fzmm.styled-flow-layout>
    </components>

    <templates>
        <option-button>
            <button id="{{id}}">
                <text translate="true">fzmm.gui.history.option.{{translation}}</text>

                <sizing>
                    <horizontal method="fill">97</horizontal>
                </sizing>

                <margins>
                    <vertical>4</vertical>
                </margins>
            </button>
        </option-button>

        <left-option-list>
            <fzmm.styled-flow-layout direction="vertical">
                <children>
                    <fzmm.styled-label>
                        <text translate="true">fzmm.gui.headGallery.label.{{labelId}}</text>
                        <tooltip-text translate="true">fzmm.gui.headGallery.label.{{labelId}}.tooltip</tooltip-text>

                        <margins>
                            <vertical>4</vertical>
                        </margins>
                    </fzmm.styled-label>

                    <fzmm.styled-scroll direction="vertical">

                        <fzmm.styled-flow-layout direction="vertical" id="{{id}}">
                            <children>

                            </children>
                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>

                            <margins>
                                <right>8</right>
                            </margins>
                        </fzmm.styled-flow-layout>

                        <sizing>
                            <vertical method="fill">100</vertical>
                        </sizing>

                        <margins>
                            <bottom>20</bottom>
                        </margins>
                    </fzmm.styled-scroll>
                </children>

                <sizing>
                    <vertical method="{{vertical-sizing-method}}">{{vertical-sizing-value}}</vertical>
                </sizing>
            </fzmm.styled-flow-layout>
        </left-option-list>

        <select-tag>
            <fzmm.styled-flow-layout direction="vertical">
                <children>
                    <fzmm.styled-label id="tags-overlay-label">
                        <margins>
                            <top>3</top>
                        </margins>
                    </fzmm.styled-label>

                    <fzmm.text-box-row>
                        <id>tag-search</id>
                        <removeHorizontalMargins>true</removeHorizontalMargins>
                    </fzmm.text-box-row>

                    <button id="clear-selected-tags">
                        <text translate="true">fzmm.gui.headGallery.button.clearSelectedTags</text>

                        <sizing>
                            <horizontal method="fill">100</horizontal>
                        </sizing>
                    </button>

                    <fzmm.styled-scroll direction="vertical" id="tag-scroll">
                        <fzmm.styled-flow-layout direction="ltr-text-flow" id="minecraft-heads-tags-ltr">
                            <children>

                            </children>

                            <vertical-alignment>top</vertical-alignment>
                            <horizontal-alignment>center</horizontal-alignment>

                            <sizing>
                                <horizontal method="fill">100</horizontal>
                            </sizing>

                            <gap>4</gap>
                        </fzmm.styled-flow-layout>

                        <sizing>
                            <horizontal method="fill">100</horizontal>
                            <vertical method="fill">60</vertical>
                        </sizing>
                    </fzmm.styled-scroll>
                </children>

                <horizontal-alignment>center</horizontal-alignment>
                <vertical-alignment>top</vertical-alignment>

                <sizing>
                    <horizontal method="fill">80</horizontal>
                </sizing>

                <gap>5</gap>
                <padding>
                    <all>5</all>
                </padding>
                <surface>
                    <panel dark="true"/>
                </surface>

                <z-index>500</z-index>
            </fzmm.styled-flow-layout>
        </select-tag>
    </templates>
</owo-ui>